# Required Ladok3 configuration
# 

# Ladok3 database name
LADOK3_DATABASE=

# Ladok3 database user
LADOK3_USERNAME=

# Ladok3 password of the user
LADOK3_PASSWORD=

# Required Mecenat configuration
#

# The username for the Mecenat SFTP server
MECENAT_USERNAME=

# The password of the Mecenat user
MECENAT_PASSWORD=

# The Mecenat customer number
MECENAT_CUSTOMERNR=

# Optional configuration of where to put copy of latest mecenat file.
# WIRETAP_OUTPUT_DIR=/opt/data/mecenat